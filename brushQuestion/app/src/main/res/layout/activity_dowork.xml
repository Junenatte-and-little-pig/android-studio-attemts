<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center"
	android:orientation="vertical"
	tools:context=".doworkActivity">
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="10dp"
		android:layout_margin="10dp">
		<TextView
			android:id="@+id/cal_1"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:gravity="center"
			android:text="1"
			android:textSize="36dp" />
		<TextView
			android:id="@+id/op"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:gravity="center"
			android:text="+"
			android:textSize="36dp" />
		<TextView
			android:id="@+id/cal_2"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:gravity="center"
			android:text="2"
			android:textSize="36dp" />
		<TextView
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:gravity="center"
			android:text="="
			android:textSize="36dp" />
		<EditText
			android:id="@+id/result"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:textSize="36dp"/>
		<ImageView
			android:id="@+id/rw"
			android:layout_width="50dp"
			android:layout_height="50dp" />
	</LinearLayout>
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="5dp"
		android:layout_margin="5dp">
		<Button
			android:id="@+id/btn_1"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="1"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_2"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="2"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_3"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="3"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_4"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="4"
			android:textSize="16dp" />
	</LinearLayout>
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="5dp"
		android:layout_margin="5dp">
		<Button
			android:id="@+id/btn_5"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="5"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_6"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="6"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_7"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="7"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_8"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="8"
			android:textSize="16dp" />
	</LinearLayout>
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="5dp"
		android:layout_margin="5dp">
		<Button
			android:id="@+id/btn_9"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="9"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_0"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="0"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_B"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="B"
			android:textSize="16dp" />
		<Button
			android:id="@+id/btn_C"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:text="C"
			android:textSize="16dp" />
	</LinearLayout>
	<Button
		android:id="@+id/submit"
		android:layout_width="100dp"
		android:layout_height="60dp"
		android:text="submit"
		android:textSize="16dp"
		android:textAllCaps="false"/>
</LinearLayout>